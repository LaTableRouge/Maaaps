{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mps/maaaps",
  "version": "0.0.1",
  "title": "Maaaps",
  "category": "maaaps",
  "icon": "block-default",
  "description": "A bloc that display maps",
  "supports": {
    "html": false,
    "anchor": true,
    "align": true,
    "spacing": {
      "padding": true
    },
    "dimensions": {
      "aspectRatio": true
    }
  },
  "attributes": {
    "blockId": {
      "type": "string"
    },
    "post-type": {
      "type": "string",
      "default": "post"
    },
    "taxonomies": {
			"type": "array",
			"default": ["category", "post_tag"]
		},
    "taxonomy": {
      "type": "string",
      "default": null
    }
  },
  "textdomain": "maaaps",
  "keywords": [
    "mps",
    "maps",
    "leaflet",
    "maaaps"
  ],
  "viewScript": "file:./assets/view.js",
  "editorScript": "file:./assets/index.jsx",
  "editorStyle": "file:./assets/index.css",
  "style": "file:./assets/style-index.css"
}

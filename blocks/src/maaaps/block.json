{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "mps/maaaps",
  "version": "0.0.1",
  "title": "Maaaps",
  "category": "maaaps",
  "icon": "block-default",
  "description": "A bloc that display maps",
  "supports": {
    "html": false,
    "anchor": true,
    "align": true,
    "spacing": {
      "padding": true
    },
    "dimensions": {
      "aspectRatio": true
    }
  },
  "attributes": {
    "blockId": {
      "type": "string"
    },
    "postType": {
      "type": "string",
      "default": "post"
    },
    "taxonomies": {
      "type": "array",
      "default": []
    },
    "selectedTaxonomies": {
      "type": "array",
      "default": []
    },
    "categories": {
      "type": "object",
      "default": {}
    },
    "selectedCategories": {
      "type": "array",
      "default": []
    },
    "selectedPosts": {
      "type": "array",
      "default": []
    },
    "mapTiles": {
      "type": "array",
      "default": [
        {
          "label": "CartoDB Voyager",
          "value": "https://{s}.basemaps.cartocdn.com/rastertiles/voyager/{z}/{x}/{y}{r}.png"
        },
        {
          "label": "CartoDB Positron",
          "value": "https://{s}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}{r}.png"
        },
        {
          "label": "OSM Mapnik",
          "value": "https://{s}.tile.openstreetmap.org/{z}/{x}/{y}{r}.png"
        },
        {
          "label": "Open Topo Map",
          "value": "https://{s}.tile.opentopomap.org/{z}/{x}/{y}{r}.png"
        }
      ]
    },
    "selectedMapTiles": {
      "type": "string",
      "default": "https://{s}.basemaps.cartocdn.com/rastertiles/voyager/{z}/{x}/{y}{r}.png"
    },
    "selectedMarkerColor": {
      "type": "string",
      "default": "#2b82cb"
    },
    "selectedClusterColor": {
      "type": "string",
      "default": "#FCA311"
    },
    "selectedSearchColor": {
      "type": "string",
      "default": "#6ee9a2"
    },
    "selectedPrimaryColor": {
      "type": "string",
      "default": "#ffed00"
    },
    "selectedSecondaryColor": {
      "type": "string",
      "default": "#2c4965"
    },
    "selectedGeolocationColor": {
      "type": "string",
      "default": "#2c4965"
    },
    "isClustered": {
      "type": "boolean",
      "default": true
    },
    "isGeolocated": {
      "type": "boolean",
      "default": true
    },
    "selectedDisplayType": {
      "type": "string",
      "default": "full"
    },
    "displaySearch": {
      "type": "boolean",
      "default": true
    },
    "limitedSearch": {
      "type": "boolean",
      "default": false
    },
    "selectedMarkerSize": {
      "type": "string",
      "default": "40px"
    },
    "selectedMarkerClusterSize": {
      "type": "string",
      "default": "50px"
    },
    "selectedMaxZoom": {
      "type": "number",
      "default": 13
    },
    "selectedMaxMarkerZoom": {
      "type": "number",
      "default": 13
    },
    "selectedSidebarSize": {
      "type": "string",
      "default": "25cqw"
    }
  },
  "textdomain": "maaaps",
  "keywords": [
    "mps",
    "maps",
    "leaflet",
    "maaaps"
  ],
  "viewScript": "file:./assets/view.js",
  "editorScript": "file:./assets/index.jsx",
  "editorStyle": "file:./assets/index.css",
  "style": "file:./assets/style-index.css"
}
